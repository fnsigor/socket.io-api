{
  "compilerOptions": {
      /* Language and Environment */
      "target": "es2020",
      "lib": ["es6"],
      /* Modules */
      "module": "commonjs",
      "rootDir": "src",
      "resolveJsonModule": true,
      "allowJs": false,
      "outDir": "dist",
      "esModuleInterop": true ,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
  }
}
